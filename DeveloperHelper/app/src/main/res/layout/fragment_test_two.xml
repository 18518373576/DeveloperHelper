<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_dark_white">

    <TextView
        style="@style/commonTitleText"
        android:text="Fragment_two"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_with_interface"
        style="@style/CommonButtonStyle"
        android:layout_width="@dimen/dp150"
        android:text="通过接口通信"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_with_viewmodel"
        style="@style/CommonButtonStyle"
        android:layout_width="@dimen/dp150"
        android:layout_marginTop="@dimen/dp20"
        android:text="通过ViewModel通信"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_with_interface" />
</androidx.constraintlayout.widget.ConstraintLayout>