<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView13"
        style="@style/commonTitleText"
        android:text="Touch event setting"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp20"
        android:gravity="center"
        android:padding="@dimen/dp10"
        android:text="ViewGroup onInterceptTouchEvent return"
        android:textSize="@dimen/sp16"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <RadioGroup
        android:id="@+id/rg_intercept_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView14">

        <RadioButton
            android:id="@+id/rb_intercept_return_true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp10"
            android:checked="true"
            android:text="return true" />

        <RadioButton
            android:id="@+id/rb_intercept_return_false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:text="return false" />
    </RadioGroup>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp20"
        android:gravity="center"
        android:padding="@dimen/dp10"
        android:text="ViewGroup onTouchEvent return"
        android:textSize="@dimen/sp16"
        app:layout_constraintTop_toBottomOf="@+id/rg_intercept_setting" />

    <RadioGroup
        android:id="@+id/rg_vg_touch_event_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView15">

        <RadioButton
            android:id="@+id/rb_vg_touch_return_true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp10"
            android:checked="true"
            android:text="return true" />

        <RadioButton
            android:id="@+id/rb_vg_touch_return_false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:text="return false" />
    </RadioGroup>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp20"
        android:gravity="center"
        android:padding="@dimen/dp10"
        android:text="ChildView onTouchEvent return"
        android:textSize="@dimen/sp16"
        app:layout_constraintTop_toBottomOf="@+id/rg_vg_touch_event_setting" />

    <RadioGroup
        android:id="@+id/rg_cv_touch_event_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView16">

        <RadioButton
            android:id="@+id/rb_cv_touch_return_true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp10"
            android:checked="true"
            android:text="return true" />

        <RadioButton
            android:id="@+id/rb_cv_touch_return_false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:text="return false" />
    </RadioGroup>

</androidx.constraintlayout.widget.ConstraintLayout>